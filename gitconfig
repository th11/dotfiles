[user]
	email = my-email@gmail.com
	name = myName
[push]
	default = upstream
[color]
  ui = auto
[alias]
  ci = commit -v
  co = checkout
  st = status
	clear = "!git reset .; git co .; git clean -f"
	unstage = reset HEAD --
	last = log -1 HEAD
[core]
  excludesfile = ~/.gitignore
  autocrlf = input
	editor = vim
[merge]
  ff = only
[fetch]
  prune = true
[rebase]
  autosquash = true
[include]
  path = ~/.gitconfig.local
[filter "lfs"]
	clean = git-lfs clean %f
	smudge = git-lfs smudge %f
	required = true
[branch]
  autosetuprebase = always
