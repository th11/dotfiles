alias zshconfig="vim ~/.zshrc"
alias gcm='git commit -m'
alias gcma='git commit --amend'
alias gst='git status'
alias gd='git diff'
alias gaa='git add -A'
alias be='bundle exec'
alias gl='git log'
alias glb='git branch --sort="-committerdate" --format="%(color:green)%(committerdate:relative)%(color:reset) %(refname:short)"'
alias gls='git log --pretty=format:"%h - %an, %ar : %s"'
alias mkdir='mkdir -p'
alias e='$EDITOR'
alias v='$VISUAL'
alias b='bundle'
alias damn='fuck'
alias rm='rm -i'
alias weather='curl -4 http://wttr.in/San_Fransisco'
alias myip='curl ipinfo.io/ip'
alias start_zdi='~/zdi-restart.sh'
alias reload='echo "reloading config ~/.zshrc" && source ~/.zshrc'
alias clean_branch='gb | grep -v master | xargs git branch -D'
alias folder-size='echo "du -hd 0"; du -hd 0'
alias folder-sizes='echo "du -hs *"; du -hs *'
alias printenv='printenv | sort'
alias ..='cd ..'
alias sublime='/Applications/Sublime\ Text.app/Contents/SharedSupport/bin/subl'
alias ll='ls -alh'
alias git-ignore-local='git update-index --assume-unchanged'
alias h=history
